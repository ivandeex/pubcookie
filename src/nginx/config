ngx_addon_name=ngx_http_pubcookie_module
HTTP_FILTER_MODULES="$HTTP_FILTER_MODULES ngx_http_pubcookie_module"
pubcookie_inc_dir=$(readlink -f $ngx_addon_dir/..)
NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_pubcookie_module.c $ngx_addon_dir/ngx_http_pubcookie_library.c"
NGX_ADDON_DEPS="$NGX_ADDON_DEPS $ngx_addon_dir/ngx_http_pubcookie.h"
CORE_INCS="$CORE_INCS $ngx_addon_dir $pubcookie_inc_dir"
